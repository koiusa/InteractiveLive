        },
        {
            "m_Id": "36c04cba756f474ea7295b1c1419f265"
        },
        {
            "m_Id": "c5fde3fc48964596b91a221a8f7b5d67"
        }
    ],
    "synonyms": [],
    "m_Precision": 0,
    "m_PreviewExpanded": true,
    "m_PreviewMode": 0,
    "m_CustomColors": {
        "m_SerializableColors": []
    },
    "m_SourceType": 1,
    "m_FunctionName": "ripple",
    "m_FunctionSource": "",
    "m_FunctionBody": "                float2 uv = _UV;\r\n\r\n                // 1pxあたりの単位を計算する\r\n                float du = 1.0 / _TextureSize.x;\r\n                float dv = 1.0 / _TextureSize.y;\r\n                float2 duv = float2(du, dv) * _DeltaUV;\r\n\r\n                // 現在の位置のテクセルをフェッチ\r\n                float2 c = tex2D(_SelfTexture2D, uv);\r\n\r\n                //波動方程式\r\n                //h(t + 1) = 2h + c(h(x + 1) + h(x - 1) + h(y + 1) + h(y - 1) - 4h) - h(t - 1)\r\n                //今回、h(t + 1)は次のフレームでの波の高さを表す\r\n                //R,Gをそれぞれ高さとして使用\r\n                float k = (2.0 * c.r) - c.g; //2h - h(t - 1) を先に計算\r\n                float p = (k + _S2 * ( //_S2は係数 位相の変化する速度\r\n                    tex2D(_SelfTexture2D, uv + duv.x).r +\r\n                    tex2D(_SelfTexture2D, uv - duv.x).r +\r\n                    tex2D(_SelfTexture2D, uv + duv.y).r +\r\n                    tex2D(_SelfTexture2D, uv - duv.y).r - 4.0 * c.r)\r\n                ) * _Attenuation; //減衰係数\r\n\r\n                // 現在の状態をテクスチャのR成分に、ひとつ前の（過去の）状態をG成分に書き込む。\r\n                WaveVector = float4(p, c.r, 0, 0);"
}

